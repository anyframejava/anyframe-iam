<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:config="http://www.anyframejava.org/schema/service"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
						http://www.anyframejava.org/schema/service
						http://www.anyframejava.org/schema/service/anyframe-core-service-2.0.xsd">

	<bean name="idGenerationServiceGroup"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixGroup" />
		<config:configuration block-size="1" table="ids"
			key-table="GROUPS" />
	</bean>

	<bean name="mixPrefixGroup"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="GRP-" />
		<property name="cipers" value="4" />
		<property name="fillChar" value="0" />
	</bean>
	
	<bean name="idGenerationServiceRole"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixRole" />
		<config:configuration block-size="1" table="ids"
			key-table="ROLES" />
	</bean>
	
	<bean name="mixPrefixRole"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="ROLE-" />
		<property name="cipers" value="5" />
		<property name="fillChar" value="0" />
	</bean>

	<bean name="idGenerationServiceResourccesMethod"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixResourceMethod" />
		<config:configuration block-size="1" table="ids"
			key-table="RESOURCE_METHOD" />
	</bean>
	
	<bean name="mixPrefixResourceMethod"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="mtd_" />
		<property name="cipers" value="6" />
		<property name="fillChar" value="0" />
	</bean>

	<bean name="idGenerationServiceResourccesPointCut"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixResourcePointCut" />
		<config:configuration block-size="1" table="ids"
			key-table="RESOURCE_PNC" />
	</bean>
	
	<bean name="mixPrefixResourcePointCut"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="pnc_" />
		<property name="cipers" value="6" />
		<property name="fillChar" value="0" />
	</bean>

	<bean name="idGenerationServiceResourccesURL"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixResourceURL" />
		<config:configuration block-size="1" table="ids"
			key-table="RESOURCE_URL" />
	</bean>
	
	<bean name="mixPrefixResourceURL"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="web_" />
		<property name="cipers" value="6" />
		<property name="fillChar" value="0" />
	</bean>
	
	<bean name="idGenerationServiceRestrictedTimes"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="strategy" ref="mixPrefixRestrictedTimes" />
		<config:configuration block-size="1" table="ids"
			key-table="TIMES" />
	</bean>
	
	<bean name="mixPrefixRestrictedTimes"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="TIME-" />
		<property name="cipers" value="5" />
		<property name="fillChar" value="0" />
	</bean>
	
</beans>
