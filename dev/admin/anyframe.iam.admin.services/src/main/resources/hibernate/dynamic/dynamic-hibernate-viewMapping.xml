<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dynamic-hibernate PUBLIC "-//ANYFRAME//DTD DYNAMIC-HIBERNATE//EN"
"http://www.anyframejava.org/dtd/anyframe-dynamic-hibernate-mapping-4.0.dtd">
<dynamic-hibernate>
	<query name="findViewMappings">
		<![CDATA[
		FROM ViewResourcesMapping viewMapping 		
		WHERE
			viewMapping.id.viewResourceId = :viewResourceId
		]]>
	</query>
</dynamic-hibernate>